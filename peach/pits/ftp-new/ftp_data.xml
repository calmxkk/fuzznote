    <!--
          RFC 959
    -->
    <!-- USER [username]: login with the specified username -->
    <DataModel name="DataUSER">
        <String value="USER " />
        <String value="" />
        <String value="calmxkk" />
        <String value="\r\n" />
    </DataModel>

    <!-- PASS [password]: login with the specified password -->
    <DataModel name="DataPASS">
        <String value="PASS " />
        <String value="" />
        <String value="123456" />
        <String value="\r\n" />
    </DataModel>

